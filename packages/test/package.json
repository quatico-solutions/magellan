        "@quatico/magellan-shared": "^0.0.1",
        "@types/node": "16",
        "tslib": "^2.3.1",
        "typescript": "~4.7.4"
    },
    "engines": {
        "node": ">=16.x",
        "yarn": ">=1.20.x"
    }
}