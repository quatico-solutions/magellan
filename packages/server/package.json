{
    "name": "@quatico/magellan-server",
    "description": "Middleware for remote service execution with the @quatico/websmith-compiler",
    "version": "0.6.1",
    "author": "Quatico Solutions AG",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/quatico-solutions/magellan"
    },
    "files": [
        "LICENSE",
        "README.md",
        "lib"
    ],
    "scripts": {
        "clean": "rimraf lib coverage",
        "lint": "eslint \"{src,test,addons}/**/*.ts\" --color",
        "lint:fix": "eslint \"{src,test,addons}/**/*.ts\" --fix",
        "build": "tsc",
        "watch": "tsc --watch",
        "dist": "cross-env-shell NODE_ENV=production \"yarn clean && yarn test && yarn build\"",
