        "test:watch": "jest --watch",
        "test:update-snapshots": "yarn test -u",
        "publish-npm": "npm publish"
    },
    "dependencies": {
        "@quatico/magellan-shared": "^0.6.1",
        "cookie-parser": "1.4.x",
        "cors": "2.8.5",
        "express": "4.18.x",
        "form-data": "^4.0.0",
        "http-errors": "1.6.x",
        "morgan": "1.9.x",
        "multer": "1.4.5-lts.1",
        "node-fetch": "^2.6.11",
        "tslib": "^2.5.0"
    },
    "devDependencies": {
        "@swc/core": "^1.3.57",
        "@swc/jest": "^0.2.26",
        "@types/cookie-parser": "^1.4.3",
